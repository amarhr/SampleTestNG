<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<listeners>
		<listener class-name="sample.listener.Listener"></listener>
	</listeners>
	<test thread-count="5" name="TestChrome">
		<parameter name="browserType" value="Chrome"></parameter>
		<classes>
			<class name="sample.tests.TestPriority" />
		</classes>
	</test>
	<test thread-count="5" name="TestIE">
		<parameter name="browserType" value="IE"></parameter>
		<classes>
			<class name="sample.tests.TestPriority" />
		</classes>
	</test>
</suite>
